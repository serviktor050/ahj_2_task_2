{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions":["error","ignorePackages"],
        "array-callback-return": 0,
        "no-unused-expressions": 0,
        "no-param-reassign": 0

    }
}
